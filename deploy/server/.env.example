# Required: change this before running in any real environment.
POSTGRES_PASSWORD=change-me-strong-password

# Optional Postgres settings.
POSTGRES_USER=rtserver
POSTGRES_DB=rtserver

# Server container image and tag.
# Local build workflow:
# SERVER_IMAGE=rt-server
# SERVER_VERSION=latest
#
# Docker Hub workflow:
# SERVER_IMAGE=iwismer/rt-server
# SERVER_VERSION=v0.1.0
SERVER_IMAGE=rt-server
SERVER_VERSION=latest

# Host port mapped to rt-server:8080.
SERVER_PORT=8080

# Server log verbosity.
LOG_LEVEL=info
